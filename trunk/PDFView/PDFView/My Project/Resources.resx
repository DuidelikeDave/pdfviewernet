  <data name="PageHtml" xml:space="preserve">
    <value>&lt;HTML&gt;
&lt;HEAD&gt;
&lt;TITLE&gt;PageViewer&lt;/TITLE&gt;
&lt;SCRIPT LANGUAGE="JavaScript"&gt;

htmlLink = new Array({HtmlLinkCount-1});
{HtmlLinkContent}

pageLink = new Array({PageLinkCount-1});
{PageLinkContent}

dpi = {DPI};
pageCount = {PageCount};

function getClickPosition(event){
	pos_x = event.offsetX?(event.offsetX):event.pageX-document.getElementById("mainimage").offsetLeft;
	pos_y = event.offsetY?(event.offsetY):event.pageY-document.getElementById("mainimage").offsetTop;
	myimg = document.getElementById("mainimage");
	imgScaleFactor = myimg.width / myimg.naturalWidth;
	actualDPI = (imgScaleFactor * dpi);
	positionScaleFactor = 72 / actualDPI;
	clickX = parseInt(pos_x * positionScaleFactor);
	clickY = parseInt(pos_y * positionScaleFactor);
	//Flip the y coordinate since postscript uses lower left corner as 0,0
	psHeight = parseInt(myimg.height * positionScaleFactor);
	clickY = psHeight - clickY;
	goToPageLink(clickX,clickY);
	goToHtmlLink(clickX,clickY);
}

function goToPageLink(xPos,yPos) {
	var x=0;
	for (x=0; x&lt;pageLink.length; x++)
	{		
	if (pageLink[x][0] == parseInt(document.getElementById("currentPage").value)) {
			if (xPos &gt;= pageLink[x][2] &amp;&amp; xPos &lt;= pageLink[x][4] &amp;&amp; yPos &gt;= pageLink[x][3] &amp;&amp; yPos &lt;= pageLink[x][5]) {
				changeImage(pageLink[x][1]);
			}
		}
	}
}

function goToHtmlLink(xPos,yPos) {
	var x=0;
	for (x=0; x&lt;htmlLink.length; x++)
	{		
	if (htmlLink[x][0] == parseInt(document.getElementById("currentPage").value)) {
			if (xPos &gt;= htmlLink[x][2] &amp;&amp; xPos &lt;= htmlLink[x][4] &amp;&amp; yPos &gt;= htmlLink[x][3] &amp;&amp; yPos &lt;= htmlLink[x][5]) {
				window.open(htmlLink[x][1], "_blank");
			}
		}
	}
}

function changeImage(pagenumber)
{
	if (pagenumber &gt; pageCount) {pagenumber = pageCount;}
	if (pagenumber &lt; 1) {pagenumber = 1;}
	filename = 'images/page' + pagenumber + '.png';
	if (pagenumber &lt;= pageCount &amp;&amp; pagenumber &gt;= 1) {
		document.getElementById('currentPage').value = pagenumber;
		if (pagenumber == 1) {
			parent.pagesize.document.getElementById('searchPage').value = 0;
		} else {
			parent.pagesize.document.getElementById('searchPage').value = pagenumber;
		}
		document.images['mainimage'].src = filename;
	}
	parent.pagesize.document.getElementById('pageNumberTextBox').value = pagenumber;
}
 
&lt;/script&gt;
&lt;/HEAD&gt;
&lt;BODY bgcolor="#999999"&gt;&lt;center&gt;&lt;img id="mainimage" src="images/page1.png" width="100%" onclick="getClickPosition(event)"&gt;&lt;/center&gt;&lt;input type="hidden" id="currentPage" value="1" /&gt;&lt;/BODY&gt;
&lt;/HTML&gt;</value>
  </data>
  <data name="PagesizeHtml" xml:space="preserve">
    <value>&lt;HTML&gt;
&lt;HEAD&gt;
&lt;TITLE&gt;PageSize&lt;/TITLE&gt;
&lt;SCRIPT LANGUAGE="JavaScript"&gt;

pageContent = new Array({PageCount-1});
{PageContent}

pageCount = {PageCount};
zoomSize = 100;

function fitScreen()
{
	parent.pageviewer.document.images['mainimage'].style.height = '100%';
	parent.pageviewer.document.images['mainimage'].style.width = 'auto';
	zoomSize = 100;
}
function fitWidth()
{
	parent.pageviewer.document.images['mainimage'].style.height = 'auto';
	parent.pageviewer.document.images['mainimage'].style.width = '100%';
	zoomSize = 100;
}
function fitActual()
{
	parent.pageviewer.document.images['mainimage'].style.height = 'auto';
	parent.pageviewer.document.images['mainimage'].style.width = 'auto';
	zoomSize = 100;
}
function decreaseZoom()
{
	zoomSize = zoomSize -5;	
	adjustZoom(zoomSize);
}
function increaseZoom()
{
	zoomSize = zoomSize +5;
	adjustZoom(zoomSize);	
}
function adjustZoom(zoom) {
	parent.pageviewer.document.images['mainimage'].style.width = zoom + '%';
	parent.pageviewer.document.images['mainimage'].style.height = 'auto';
}
function changeImage(pagenumber)
{
	if (pagenumber &gt; pageCount) {pagenumber = pageCount;}
	if (pagenumber &lt; 1) {pagenumber = 1;}
	filename = 'images/page' + pagenumber + '.png';
	if (pagenumber &lt;= pageCount &amp;&amp; pagenumber &gt;= 1) {
		parent.pageviewer.document.getElementById('currentPage').value = pagenumber;
		if (pagenumber == 1) {
			document.getElementById('searchPage').value = 0;
		} else {
			document.getElementById('searchPage').value = pagenumber;
		}
		parent.pageviewer.document.images['mainimage'].src = filename;
	}
	updatePageStatus(pagenumber);
}
function updatePageStatus(pagenumber) {
	document.getElementById('pageNumberTextBox').value = pagenumber;
}
function nextPage()
{
	newPageNumber = parseInt(parent.pageviewer.document.getElementById('currentPage').value) + 1;
	if (newPageNumber &lt;= pageCount) {
		changeImage(newPageNumber);
	} else {
		changeImage(pageCount);
	}
}
function previousPage()
{
	newPageNumber = parseInt(parent.pageviewer.document.getElementById('currentPage').value) - 1;
	if (newPageNumber &gt;= 1) {
		changeImage(newPageNumber);
	} else {
		changeImage(1);
	}
}
function gotoPage() {
	changeImage(document.getElementById('pageNumberTextBox').value)
}
function searchNext() {
	searchtext = document.getElementById('searchTextBox').value;
	var x=0;
	for (x=0; x&lt;pageCount; x++)
	{
		if (pageContent[x][1].toUpperCase().indexOf(searchtext.toUpperCase()) &gt;= 0 &amp;&amp; pageContent[x][0] &gt; document.getElementById('searchPage').value) {
			updatePageStatus(pageContent[x][0]);
			gotoPage()
			break;
		}
	} 
}
function searchFirst() {
	searchtext = document.getElementById('searchTextBox').value;
	document.getElementById('searchPage').value = 0;
	var x=0;
	for (x=0; x&lt;pageCount; x++)
	{
		if (pageContent[x][1].toUpperCase().indexOf(searchtext.toUpperCase()) &gt;= 0 &amp;&amp; pageContent[x][0] &gt; document.getElementById('searchPage').value) {
			updatePageStatus(pageContent[x][0]);
			gotoPage()
			break;
		}
	} 
}
function searchPrevious() {
	pageContent.reverse();	
	searchtext = document.getElementById('searchTextBox').value;
	var x=0;
	for (x=0; x&lt;pageCount; x++)
	{
		if (pageContent[x][1].toUpperCase().indexOf(searchtext.toUpperCase()) &gt;= 0 &amp;&amp; pageContent[x][0] &lt; document.getElementById('searchPage').value) {
			updatePageStatus(pageContent[x][0]);
			gotoPage()
			break;
		}
	}
	pageContent.reverse(); 
}

&lt;/script&gt;
&lt;/HEAD&gt;
&lt;BODY bgcolor="#FFFFFF"&gt;
&lt;table bgcolor="#FFFFFF"&gt;
&lt;tr&gt;
&lt;td valign=center&gt;Go to page:&lt;/td&gt;
&lt;td valign=center&gt;&lt;input id="pageNumberTextBox" onkeydown="if (event.keyCode == 13) gotoPage()" type=text size=5 value="1"&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/Previous.png" alt="Previous Page" onclick="previousPage()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/Next.png" alt="Next Page" onclick="nextPage()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/FitToScreen.png" alt="Fit To Screen" onclick="fitScreen()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/FitToWidth.png" alt="Fit To Width" onclick="fitWidth()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/ActualSize.png" alt="Actual Size" onclick="fitActual()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/ZoomIn.png" alt="Search Next" onclick="increaseZoom()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/ZoomOut.png" alt="Search Next" onclick="decreaseZoom()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;input id="searchTextBox" type=text size=30&gt;&lt;input type="hidden" id="searchPage" value="0" /&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/Search.png" alt="Search From Beginning" onclick="searchFirst()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/SearchPrevious.png" alt="Search Previous" onclick="searchPrevious()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&lt;img src="images/SearchNext.png" alt="Search Next" onclick="searchNext()"/&gt;&lt;/td&gt;
&lt;td valign=center&gt;&amp;nbsp;&amp;nbsp;&lt;a href="images/{DocumentName}" TARGET = "_blank"&gt;{DocumentName}&lt;/a&gt;&lt;/td&gt;
&lt;tr/&gt;
&lt;table/&gt;
&lt;hr/&gt;
&lt;/BODY&gt;
&lt;/HTML&gt;</value>

    <value>&lt;HTML&gt;
&lt;HEAD&gt;
&lt;TITLE&gt;PageIndex&lt;/TITLE&gt;
&lt;SCRIPT LANGUAGE="JavaScript"&gt;
function changeImage(pagenumber)
{
	pageCount = {PageCount};
	filename = 'images/page' + pagenumber + '.png';
	if (pagenumber &lt;= pageCount &amp;&amp; pagenumber &gt;= 1) {
		parent.pageviewer.document.getElementById('currentPage').value = pagenumber;
		parent.pagesize.document.getElementById('pageNumberTextBox').value = pagenumber;
		if (pagenumber == 1) {
			parent.pagesize.document.getElementById('searchPage').value = 0;
		} else {
			parent.pagesize.document.getElementById('searchPage').value = pagenumber;
		}	
		parent.pageviewer.document.images['mainimage'].src = filename;
	}
}
&lt;/script&gt;
